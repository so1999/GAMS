<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.manager.gams.domain">
	<class name="BorrowInfo" table="M_BORROW_INFO">
		<id name="bid" type="java.lang.Integer">
			<column name="ID" length="10" />
			<generator class="native" />
		</id>
		<property name="iou" type="java.lang.String" length="10" />
		<property name="iopcno" type="java.lang.String" length="20" />
		<property name="iopcmodel" type="java.lang.String" length="20" />
		<property name="iopcsn" type="java.lang.String" length="20" />
		<property name="iopccharger" type="java.lang.String" length="10" />
		<property name="iopcplace" type="java.lang.String" length="50" />
		<property name="iopcstatus" type="java.lang.String" length="20" />
		<property name="ioperson" type="java.lang.String" length="10" />
		<property name="ioyon" type="java.lang.String" length="10" />
		<property name="ioindate" type="java.lang.String" length="10" />
		<property name="ioopdate" type="java.lang.String" length="50" />
		<property name="ioopperson" type="java.lang.String" length="50" />
		<property name="ioremark" type="java.lang.String" length="200" />
	</class>
</hibernate-mapping>